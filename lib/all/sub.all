(include all
    lib/all/inc.all
    lib/all/dec.all
    lib/all/eq.all
    lib/all/ret.all
    lib/all/lr.all)

; sub(x, y) = x, if y = 0
; sub(x, y) = sub(x, y+1) + 1
; sub(x, y) = sub(x, y-1) - 1
(define (sub x y)
    (if (eq y 0)
        (ret x)
        (if (lr y 0)
            (inc (sub x (inc y)))
            (dec (sub x (dec y))))))
